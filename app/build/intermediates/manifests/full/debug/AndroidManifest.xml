<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sarveshchavan777.triviaquiz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name="sarveshchavan777.triviaquiz.HomeScreen"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="sarveshchavan777.triviaquiz.PlayAgain"
            android:screenOrientation="portrait" />
        <activity
            android:name="sarveshchavan777.triviaquiz.GameWon"
            android:screenOrientation="portrait" />
        <activity
            android:name="sarveshchavan777.triviaquiz.Time_Up"
            android:screenOrientation="portrait" />
        <activity
            android:name="sarveshchavan777.triviaquiz.MainGameActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name="sarveshchavan777.triviaquiz.TestMainActivity" />
    </application>

</manifest>